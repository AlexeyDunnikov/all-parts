                    <div class="profile__card-text">
                      <p class="profile__card-num">
                      {{num}}
                    </p>
                    <p class="profile__card-date">
                      {{month}}/{{year}}
                    </p>
                    </div>
                      <button class="profile__card__delete-form__btn" data-card-id="{{id}}">
                        <svg class="profile__card__delete-form__btn-img" width="26" height="26" viewBox="0 0 26 26" fill="none" xmlns="http://www.w3.org/2000/svg">
